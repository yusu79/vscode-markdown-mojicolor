{
  "name": "markdown-mojicolor",
  "displayName": "Markdown MojiColor",
  "description": "VScode extension to enable changing Markdown text color",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "private":true,
  "contributes": {
    "commands": [
      {
        "command": "markdown-mojicolor.helloWorld",
